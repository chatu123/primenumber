#include<stdio.h>
int main()
{
    int n,i,flag=0;
    printf("enter the number");
    scanf("%d",&n);
    for(i=2;i<=n;i++)
    {
        if(n/i==0)
        {
            printf("the number is prime");
            break;
        }
        else
        {
            printf("the number is not prime");
            break;
        }
    }
    return 0;
}

